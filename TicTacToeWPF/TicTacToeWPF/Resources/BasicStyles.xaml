    <Style x:Key="TicTacButton" TargetType="{x:Type Button}" BasedOn="{x:Null}">
        <Setter Property="Height" Value="78"/>
        <Setter Property="Width" Value="78"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="IsHitTestVisible" Value="{Binding IsBlockEnabled}"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding IsWinningBlock}" Value="True">
                <Setter Property="Background" Value="Green"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding PlayerIcon}" Value="X">
                <Setter Property="Content" Value="{StaticResource XPlayerIcon}"/>
                <Setter Property="Padding" Value="0 0 10 7"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding PlayerIcon}" Value="O">
                <Setter Property="Content" Value="{StaticResource OPlayerIcon}"/>
                <Setter Property="Padding" Value="3 2 8 5"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding PlayerIcon}" Value="{x:Null}">
                <Setter Property="Content" Value=""/>
                <Setter Property="Padding" Value="0"/>
            </DataTrigger>
        </Style.Triggers>
    </Style>
</ResourceDictionary>